{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "info",
	"description": "Server error response",
	"type": "object",
	"required": ["HAPI","status"],
	"properties": {
		"HAPI": {
			"description": "HAPI Version",
			"type": "string",
			"pattern": "^1.1$"
		},
		"status": {
			"description": "Request status",
			"type": "object",
			"required": ["code","message"],
			"properties": {
				"code": {
					"description": "HAPI status code",
					"type": "integer",
					"enum": [1400,1401,1402,1403,1404,1405,1406,1407,1408,1409]
				},
				"message": {
					"description": "HAPI status message",
					"type": "string"
				}
			}
		}
	}
}